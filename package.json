{
  "name": "dclinic-contracts",
  "version": "1.0.0",
  "description": "Truffle project for the development of the DCL Token Smart Contracts",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "truffle test --network test",
    "deploy": "truffle migrate",
    "deploy:ganache": "truffle migrate",
    "deploy:mumbai_testnet": "truffle migrate --f 3 --to 3 --network mumbai_testnet",
    "deploy:polygon_mainnet": "truffle migrate --f 3 --to 3 --network polygon_mainnet",
    "deploy:mainnet": "truffle migrate --f 2 --to 2 --network mainnet",
    "deploy:ropsten": "truffle migrate --f 2 --to 2 --network ropsten"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nonceblox/dclinic-contracts.git"
  },
  "keywords": [
    "DCL",
    "Dclinic",
    "ERC20",
    "Token",
    "Polygon",
    "Ethereum"
  ],
  "author": "Nonceblox",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nonceblox/dclinic-contracts/issues"
  },
  "homepage": "https://github.com/nonceblox/dclinic-contracts#readme",
  "dependencies": {
    "@openzeppelin/contracts": "^3.4.0",
    "@openzeppelin/contracts-upgradeable": "^3.4.0",
    "@truffle/hdwallet-provider": "^1.4.0",
    "dotenv": "^10.0.0",
    "truffle-assertions": "^0.9.2",
    "web3": "^1.3.6"
  }
}
